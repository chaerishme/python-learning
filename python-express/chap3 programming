# 15
num = int(input("정수를 입력하시요.:"))
if num % 15 == 0 :
  print("Python Express")
elif num % 3 == 0 :
  print("Python")
elif num % 5 == 0 :
  print("Express")
  
  
# 16
pH = int(input("pH를 입력하시오.:"))
if pH > 7 :
  print("알칼리입니다.")
elif pH < 7 :
  print("산입니다.")
else :
  print("중성입니다.")
